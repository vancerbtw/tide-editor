{
  "name": "tide-app",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Vance McHargue",
    "email": "supremevance@gmail.com",
    "url": "https://blazerepo.com"
  },
  "build": {
    "appId": "com.vancer.tide",
    "productName": "TIDE",
    "copyright": "Copyright Â© 2019 ${vancer}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "babel-plugin-wildcard": "^5.0.0",
    "bootstrap": "^4.3.1",
    "brace": "^0.11.1",
    "custom-electron-titlebar": "^3.1.0",
    "electron-is-dev": "^1.1.0",
    "jquery": "^3.4.1",
    "react": "^16.10.2",
    "react-ace": "^7.0.4",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-scripts": "3.2.0",
    "react-treebeard": "^3.2.4"
  },
  "main": "public/electron.js",
  "scripts": {
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "yarn build -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.12",
    "@rescripts/rescript-env": "^0.0.10",
    "concurrently": "^4.1.0",
    "electron": "^4.0.6",
    "electron-builder": "^21.2.0",
    "typescript": "^3.6.3",
    "wait-on": "^3.2.0"
  }
}
